{
    "name": "gchernikov/request-resolver-bundle",
    "description": "Symfony bundle for constructing request dto's and resolving them as Controller arguments",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^5|^6",
        "symfony/validator": "^5|^6",
        "symfony/serializer": "^5|^6",
        "zircote/swagger-php": "^4.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "GChernikov\\RequestResolverBundle\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gennady Chernikov",
            "email": "chernikovg@gmail.com"
        }
    ]
}
